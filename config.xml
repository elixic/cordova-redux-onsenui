<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.helloreduxonsenui" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloReduxOnsenui</name>
    <description>
        A sample Apache Cordova application that hosts a simple redux state backed onsenui.
    </description>
    <author email="isaac@arodreducation.com" href="https://www.ardoreducation.com">
        Isaac @ Ardoreducation
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-network-information" />
    <access origin="*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="^5.0.0" />
</widget>
